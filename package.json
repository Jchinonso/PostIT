{
  "name": "post-it",
  "version": "0.0.1",
  "description": "postit is a simple application that post notification to members of a group",
  "main": "app.js",
  "scripts": {
    "postinstall": "NODE_ENV=production && npm run build",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node tools/build.js",
    "start": "babel-node tools/distServer.js",
    "start:dev": "babel-watch server.js",
    "setup:test": "npm run migrations && NODE_ENV=test sequelize db:seed:all",
    "transpile": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./tests,./coverage --copy-files",
    "migrations": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate ",
    "test": "npm run setup:test && NODE_ENV=test nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/tests/**/*.js --timeout 15000",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jchinonso/PostIt.git"
  },
  "author": "johnson chinonso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jchinonso/PostIt.git/issues"
  },
  "homepage": "https://github.com/Jchinonso/PostIt.git#readme",
