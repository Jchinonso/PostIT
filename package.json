{
  "name": "post-it",
  "version": "0.0.1",
  "description": "postit is a simple application that post notification to members of a group",
  "main": "app.js",
  "scripts": {
    "start": "npm run transpile && node dist/server.js",
    "start:dev": "babel-watch server.js",
    "transpile": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./tests,./coverage --copy-files",
    "migrations": "sequelize db:migrate",
    "test": "NODE_ENV=test nyc mocha server/tests/**/*.js --timeout 15000",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jchinonso/PostIt.git"
  },
  "author": "johnson chinonso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jchinonso/PostIt.git/issues"
  },
  "homepage": "https://github.com/Jchinonso/PostIt.git#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-watch": "^2.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "faker": "^4.1.0",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "omit": "^1.0.1",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.0.0",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "sinon": "^2.3.4"
  }
}
